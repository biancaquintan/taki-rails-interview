<%= form_for businesses, :url => business_index_path, :html => { :multipart => true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <br>
  <br>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <br>
  <br>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <br>
  <br>


  <%= submit_tag "Add", class: "btn btn-primary" %>
<% end %>